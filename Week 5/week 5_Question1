#include<iostream>
#include<algorithm>
using namespace std;

int main(){
    int t;
    cin>> t;
    while(t>0){
        int n;
        cin>>n;
        int arr[n];
        int comparisions = 0, swaps = 0;
        int i , j , k, min_idx, temp;
        for(i=0;i<n;i++){
            cin>>arr[i];   // Taking Array Input
        }
        t--;
    }
}